@model IPartialModel

<div class="dc-api-check-box @Model.Cls" @Model.Attrs>
    <h2 class="dcg-h2">
        API
    </h2>
    <div class="dc-api-check-box__wrapper">
        <div class="dc-api-check-box__input">
            <input type="checkbox" id="gecko" name="gecko" checked>
            <label for="gecko">Gecko</label>
        </div>
        <div class="dc-api-check-box__input">
            <input type="checkbox" id="market-cap" name="market-cap">
            <label for="market-cap">MarketCap</label>
        </div>
    </div>
</div>